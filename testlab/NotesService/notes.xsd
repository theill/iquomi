<?xml version="1.0"?>
<xs:schema id="notes" targetNamespace="http://tempuri.org/notes.xsd" xmlns:mstns="http://tempuri.org/notes.xsd" xmlns="http://tempuri.org/notes.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
    <xs:element name="notes" msdata:IsDataSet="true" msdata:EnforceConstraints="False">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="note">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="title" type="xs:string" minOccurs="1" msdata:Ordinal="0" msdata:Caption="Title" />
                            <xs:element name="description" type="xs:string" minOccurs="1" msdata:Ordinal="1" msdata:Caption="Description" />
                        </xs:sequence>
                        <xs:attribute name="id" form="unqualified" type="xs:string" />
                        <xs:attribute name="type" type="xs:string" msdata:AutoIncrement="false" msdata:Caption="Type" />
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>